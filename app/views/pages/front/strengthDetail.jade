extends ../../layout

block content
	div.front-banner
		img.img-responsive(src="/images/banner_strength.jpg")
	div.body
		div.section
			div.container.ofh
				div.section-display-box
					div.item-list.item-3
						include ../../includes/side
					div.item-list.item-9.mt0
						div.detail-box
							ol.breadcrumb
								li
									a(href="/") 首页
								li
									a(href="/strength") 实力展示
								li #{strength.title}
							div.detail-body
								h3.detail-title #{strength.title}
								div.detail-content!=strength.content